#include <vector> //like an array
#include <string> //strings!
#include <iostream> //equivalent to stdio
using namespace std;

int main(){
  for(int i = 0; i < 100; i+=2){
    if( i % 7 == 0) break;
    cout << i << ' ';
  }
  cout << endl;

  vector<int> v;
  cout << v.size() << endl;
  v.push_back(17);
  cout << v.size() << endl;
}
